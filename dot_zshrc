# Alias helm
alias h2="tar -zxvf /home/gary/repos/helm-v2.16.7-linux-amd64.tar.gz -C /home/gary/repos/helm2  ; sudo rm /usr/local/bin/helm ; sudo mv /home/gary/repos/helm2/linux-amd64/helm /usr/local/bin/helm && echo 'Helm version 2 ✓' && helm version"
alias h3="tar -zxvf /home/gary/repos/helm-v3.2.1-linux-amd64.tar.gz -C /home/gary/repos/helm3 ; sudo rm /usr/local/bin/helm ; sudo mv /home/gary/repos/helm3/linux-amd64/helm /usr/local/bin/helm && echo 'Helm version 3 ✓' && helm version"
#alias helm2="/usr/bin/helm2/helm"
#alias helm3="/usr/bin/helm"

